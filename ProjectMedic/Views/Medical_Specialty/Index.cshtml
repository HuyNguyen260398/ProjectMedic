@model IEnumerable<ProjectMedic.Models.Medical_Specialty>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layout/_AdminLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MedicalSpecialty_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MedicalSpecialty_ServicePrice)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MedicalSpecialty_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MedicalSpecialty_ServicePrice)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MedicalSpecialty_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.MedicalSpecialty_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MedicalSpecialty_ID })
        </td>
    </tr>
}

</table>
